{
  "name": "cocktail-compatibility-analyzer",
  "version": "5.0.0",
  "description": "A web app to analyze your bar stock and match cocktail recipes with AI-powered search",
  "type": "module",
  "main": "server/proxy-server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "proxy": "node server/proxy-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run proxy\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "cocktail",
    "bartending",
    "recipe",
    "analyzer",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
